{
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "eslint:recommended"
    ],
    "plugins": [
       "class-property"
    ],
    "rules": {
        "quotes": [2, "single"],
        "semi": [2, "always"],
        "no-console": [ 0 ],
        "no-trailing-spaces": [ 0 ],
        "no-param-reassign": [ 0 ],
        "no-unused-expressions": [ 0 ]
    },
    "env": {
        "es6": true,
        "node": true
    }
}